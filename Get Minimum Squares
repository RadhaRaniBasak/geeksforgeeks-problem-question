class Solution {
    public int minSquares(int n) {
        int[] dp = new int[n+1];
        dp[0] = 0;
        
        for(int i = 1; i <= n; i++){
            dp[i] = Integer.MAX_VALUE;
        }
        for(int i = 1; i <= n; i++){
            for(int j = 1; j*j <= i; j++){
                int squareOfInteger = j*j;
                dp[i] = Math.min(dp[i], 1+dp[i-squareOfInteger]);
            }
        }
        return dp[n];
    }
}
